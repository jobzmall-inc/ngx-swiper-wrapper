{
  "compilerOptions": {
    "baseUrl": "",
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "inlineSources": false,
    "lib": [
      "es2015",
      "dom"
    ],
    "module": "es2015",
    "moduleResolution": "node",
    "outDir": "./dist",
    "paths": {
      "@angular/*": [
        "./node_modules/@angular/*"
      ],
      "swiper": [
        "./node_modules/swiper/dist/js/swiper.js"
      ]
    },
    "rootDir": ".",
    "strict": true,
    "sourceMap": true,
    "target": "es5",
    "typeRoots": [
      "./node_modules/@types"
    ],
    "types": [
      "node",
      "swiper"
    ]
  }
}
